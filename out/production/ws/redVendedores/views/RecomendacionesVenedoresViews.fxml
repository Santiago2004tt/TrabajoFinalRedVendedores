      </TableView>
      <TextField fx:id="txtFiltroMeGusta" layoutX="14.0" layoutY="67.0" />
      <Label layoutX="20.0" layoutY="32.0" prefHeight="18.0" prefWidth="131.0" text="Filtro por me gustas" />
      <Button fx:id="txtBuscarVendedor" layoutX="13.0" layoutY="114.0" mnemonicParsing="false" text="Buscar" />
